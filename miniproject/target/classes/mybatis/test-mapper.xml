<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<!-- namespace : DAO인터페이스의 풀네임. 패키지부터 인터페이스이름까지 모두 작성한다 -->	
<mapper namespace="com.kyerip.repository.TestDAO">

	<!-- id : 인터페이스에 선언된 메서드의 이름 -->
	<!-- resultType : 반환자료형 -->
	<!-- parameterType : 매개변수자료형 -->
	
	<select id="getVersion" resultType="string">
		select  
			banner  
				from v$version 
	</select> 
	
	<select id="selectSysdate" resultType="java.sql.Date">
		select sysdate from dual
	</select>
	
</mapper>





